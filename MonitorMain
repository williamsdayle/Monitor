package monitor;

public class Monitor {

    
    public static void main(String[] args) {
       int notas[] = new int[5];
       notas[0] = 4;
       notas[1] = 5;
       notas[2] = 10;
       notas[3] = 8;
       notas[4] = 7;
        Escola es1 = new Escola(notas,7,3);      
        Thread t1 = new Thread(es1);
        
        t1.start();
       
       
       
    }
    
}
